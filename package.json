{
  "name": "ng-vts-workspace",
  "version": "1.0.0",
  "license": "MIT",
  "description": "An Angular enterprise-class UI components",
  "engines": {
    "node": "^12.14.1 || ^14.0.0"
  },
  "scripts": {
    "start": "cross-env PORT='4200' CONTEXT_PATH='/' gulp start:dev",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI --code-coverage",
    "test:watch": "gulp test:watch --tags",
    "test:schematics": "gulp build:schematics && gulp test:schematics",
    "build": "cross-env CONTEXT_PATH='/uikit/' gulp build:release",
    "build:lib": "gulp build:library",
    "build:doc": "gulp build:site-doc-iframe",
    "publish": "npm run build:lib && npm publish publish",
    "doc": "gulp build:preview",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg components/package.json && node ./scripts/site/replace-scope-prefix.js",
    "format": "prettier --config .prettierrc --write \"components/**/*.ts\" \"scripts/site/**/*.ts\"",
    "stage-release": "ts-node --project scripts/release/tsconfig.json scripts/release/release.ts",
    "ng": "ng",
    "gulp": "gulp",
    "lint": "tslint --project components/tsconfig.lib.json -c tslint.json"
  },
  "dependencies": {
    "@angular/cdk": "^14.2.7",
    "@ui-vts/icons-angular": "^1.0.2",
